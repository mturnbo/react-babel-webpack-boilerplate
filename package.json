  "author": "Marcus E. Turnbo <marcus@emarcus.com>",
  "description": "React.js Babel Webpack Boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mturnbo/react-babel-webpack-boilerplate.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.11.0",
    "sass-loader": "^6.0.7",
    "webpack": "^3.5.5",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.0.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.1.6",
    "@storybook/react": "^3.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "main": "index.jsx",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js .",
    "test": "jest --config=jest.config.json",
    "dockerbuild": "webpack --config webpack.config.js && docker build -t react-babel-webpack-boilerplate:latest .",
    "storybook": "start-storybook -p 9091 -c .storybook"
  }
}
